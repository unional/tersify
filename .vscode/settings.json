{
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"cSpell.words": [
		"Acron",
		"jsnext",
		"libm",
		"pinst",
		"repobuddy",
		"snyk",
		"tersible",
		"Tersiblized",
		"tersify",
		"tsbuildinfo",
		"unpartial"
	],
	"eslint.enable": true,
	"json.schemas": [
		{
			"fileMatch": [
				"tsconfig.*.json"
			],
			"url": "http://json.schemastore.org/tsconfig"
		}
	],
	"markdownlint.ignore": ".markdownlintignore",
	"typescript.tsdk": "node_modules/typescript/lib"
}
