{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES2019"
    ],
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "rootDir": "ts",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "useDefineForClassFields": true
  },
  "files": [
    "ts/index.ts",
    "ts/constants.browser.ts",
    "ts/tersifyFunction.browser.ts"
  ]
}
