{
  "name": "tersify",
  "version": "0.0.0-development",
  "description": "Creates a terse representation of code",
  "homepage": "https://github.com/unional/tersify",
  "bugs": {
    "url": "https://github.com/unional/tersify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/tersify.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "browser": {
    "./cjs/constants.js": "./cjs/constants.browser.js",
    "./cjs/constants.d.ts": "./cjs/constants.browser.d.ts",
    "./cjs/constants.js.map": "./cjs/constants.browser.js.map",
    "./esm/constants.js": "./esm/constants.browser.js",
    "./esm/constants.d.ts": "./esm/constants.browser.d.ts",
    "./esm/constants.js.map": "./esm/constants.browser.js.map",
    "./ts/constants.js": "./ts/constants.browser.js",
    "./cjs/tersifyFunction.js": "./cjs/tersifyFunction.browser.js",
    "./cjs/tersifyFunction.d.ts": "./cjs/tersifyFunction.browser.d.ts",
    "./cjs/tersifyFunction.js.map": "./cjs/tersifyFunction.browser.js.map",
    "./esm/tersifyFunction.js": "./esm/tersifyFunction.browser.js",
    "./esm/tersifyFunction.d.ts": "./esm/tersifyFunction.browser.d.ts",
    "./esm/tersifyFunction.js.map": "./esm/tersifyFunction.browser.js.map",
    "./ts/tersifyFunction.ts": "./ts/tersifyFunction.browser.ts"
  },
  "typings": "esm/index.d.ts",
  "files": [
    "dist",
    "cjs",
    "esm",
    "ts"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "bundle": "webpack",
    "clean": "rimraf dist cjs esm lib libm *.tsbuildinfo",
    "coverage": "jest --coverage",
    "dc": "yarn dependency-check",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "eslint --ext=ts,js .",
    "test": "jest --reporters=default",
    "verify": "yarn lint && yarn build && yarn dependency-check && jest --coverage --reporters=default",
    "watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "acorn": "^8.6.0",
    "is-buffer": "^2.0.5",
    "unpartial": "^0.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^27.0.3",
    "codecov": "^3.8.3",
    "dependency-check": "^4.1.0",
    "eslint": "8.4.0",
    "eslint-plugin-harmony": "^6.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.4.3",
    "jest-junit": "^13.0.0",
    "jest-progress-tracker": "^3.0.1",
    "jest-validate": "^27.4.2",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.0.0",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  }
}
