{
  "name": "tersify",
  "version": "0.0.0-development",
  "description": "Creates a terse representation of code",
  "homepage": "https://github.com/unional/tersify",
  "bugs": {
    "url": "https://github.com/unional/tersify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/tersify.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "browser": {
    "./cjs/constants.js": "./cjs/constants.browser.js",
    "./cjs/constants.d.ts": "./cjs/constants.browser.d.ts",
    "./cjs/constants.js.map": "./cjs/constants.browser.js.map",
    "./esm/constants.js": "./esm/constants.browser.js",
    "./esm/constants.d.ts": "./esm/constants.browser.d.ts",
    "./esm/constants.js.map": "./esm/constants.browser.js.map",
    "./ts/constants.ts": "./ts/constants.browser.ts",
    "./cjs/tersifyFunction.js": "./cjs/tersifyFunction.browser.js",
    "./cjs/tersifyFunction.d.ts": "./cjs/tersifyFunction.browser.d.ts",
    "./cjs/tersifyFunction.js.map": "./cjs/tersifyFunction.browser.js.map",
    "./esm/tersifyFunction.js": "./esm/tersifyFunction.browser.js",
    "./esm/tersifyFunction.d.ts": "./esm/tersifyFunction.browser.d.ts",
    "./esm/tersifyFunction.js.map": "./esm/tersifyFunction.browser.js.map",
    "./ts/tersifyFunction.ts": "./ts/tersifyFunction.browser.ts"
  },
  "typings": "cjs/index.d.ts",
  "files": [
    "dist",
    "cjs",
    "esm",
    "ts"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:verify": "run-s build dependency-check size-limit",
    "bundle": "webpack",
    "clean": "rimraf dist cjs esm lib libm *.tsbuildinfo",
    "coverage": "jest --coverage",
    "dc": "yarn dependency-check",
    "dependency-check": "run-p dependency-check:unused dependency-check:missing",
    "dependency-check:missing": "dependency-check . --missing --no-dev",
    "dependency-check:unused": "dependency-check . --unused --no-dev",
    "lint": "eslint --ext=js,ts .",
    "prepublishOnly": "yarn bundle",
    "release": "npx semantic-release",
    "size-limit": "size-limit",
    "test": "run-p test:node test:swc",
    "test:node": "jest",
    "test:swc": "jest -c jest.config.swc.js",
    "verify": "run-p build:verify lint coverage",
    "watch": "jest --watch"
  },
  "dependencies": {
    "acorn": "^8.7.0",
    "is-buffer": "^2.0.5",
    "unpartial": "^0.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@swc/core": "^1.2.168",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.23.0",
    "codecov": "^3.8.3",
    "dependency-check": "^4.1.0",
    "eslint": "8.15.0",
    "eslint-plugin-harmony": "^7.0.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jest": "^28.0.0",
    "jest-junit": "^13.1.0",
    "jest-progress-tracker": "^3.0.4",
    "jest-validate": "^28.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "param-case": "^3.0.4",
    "pascal-case": "^3.1.2",
    "size-limit": "^7.0.8",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "size-limit": [
    {
      "path": "cjs/index.js",
      "limit": "5 kB"
    },
    {
      "path": "esm/index.js",
      "limit": "5 kB"
    }
  ]
}
