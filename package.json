{
  "name": "tersify",
  "version": "0.0.0-development",
  "description": "Format tersely",
  "homepage": "https://github.com/unional/tersify",
  "bugs": {
    "url": "https://github.com/unional/tersify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/tersify.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm",
    "src"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib libm",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --reporters=default",
    "verify": "npm run lint && npm run build && npm run dependency-check && jest --coverage --reporters=default",
    "watch": "jest --watch"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "acorn-bigint": "^0.3.1",
    "unpartial": "^0.5.0"
  },
  "devDependencies": {
    "@unional/devpkg-node": "^1.2.5"
  }
}
