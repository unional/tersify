{
	"extends": "@repobuddy/typescript/tsconfig/monorepo.json",
	"compilerOptions": {
		"composite": false,
		"exactOptionalPropertyTypes": false,
		"jsx": "react-jsx",
		"lib": ["DOM", "ES2019"],
		"noImplicitAny": false,
		"noPropertyAccessFromIndexSignature": false,
		"noUncheckedIndexedAccess": false,
		"noUnusedParameters": false,
		"outDir": ".nowhere",
		"rootDir": ".",
		"target": "ES2021"
	}
}
