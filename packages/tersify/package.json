{
  "name": "tersify",
  "version": "3.12.1",
  "description": "Creates a terse representation of code",
  "homepage": "https://github.com/unional/tersify/tree/main/packages/tersify",
  "bugs": {"url": "https://github.com/unional/tersify/issues"},
  "repository": {"type": "git", "url": "https://github.com/unional/tersify.git", "directory": "packages/tersify"},
  "license": "MIT",
  "author": {"name": "Homa Wong", "email": "homawong@gmail.com"},
  "sideEffects": false,
  "type": "module",
  "exports": {
    "types": "./esm/index.d.ts",
    "import": "./esm/index.js",
    "default": "./cjs/index.js"
  },
  "main": "./cjs/index.js",
  "browser": {
    "./cjs/constants.d.ts": "./cjs/constants.browser.d.ts",
    "./cjs/constants.js": "./cjs/constants.browser.js",
    "./cjs/constants.js.map": "./cjs/constants.browser.js.map",
    "./cjs/tersifyFunction.d.ts": "./cjs/tersifyFunction.browser.d.ts",
    "./cjs/tersifyFunction.js": "./cjs/tersifyFunction.browser.js",
    "./cjs/tersifyFunction.js.map": "./cjs/tersifyFunction.browser.js.map",
    "./esm/constants.d.ts": "./esm/constants.browser.d.ts",
    "./esm/constants.js": "./esm/constants.browser.js",
    "./esm/constants.js.map": "./esm/constants.browser.js.map",
    "./esm/tersifyFunction.d.ts": "./esm/tersifyFunction.browser.d.ts",
    "./esm/tersifyFunction.js": "./esm/tersifyFunction.browser.js",
    "./esm/tersifyFunction.js.map": "./esm/tersifyFunction.browser.js.map",
    "./src/constants.ts": "./src/constants.browser.ts",
    "./src/tersifyFunction.ts": "./src/tersifyFunction.browser.ts",
    "./tslib/constants.d.ts": "./tslib/constants.browser.d.ts",
    "./tslib/constants.js": "./tslib/constants.browser.js",
    "./tslib/constants.js.map": "./tslib/constants.browser.js.map",
    "./tslib/tersifyFunction.d.ts": "./tslib/tersifyFunction.browser.d.ts",
    "./tslib/tersifyFunction.js": "./tslib/tersifyFunction.browser.js",
    "./tslib/tersifyFunction.js.map": "./tslib/tersifyFunction.browser.js.map"
  },
  "types": "./esm/index.d.ts",
  "files": ["dist", "cjs", "esm", "src", "!src/**/*.spec.ts"],
  "scripts": {
    "build": "run-p build:cjs build:esm build:tslib",
    "build:cjs": "tsc -p tsconfig.cjs.json && ncp package.cjs.json cjs/package.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:tslib": "tsc -p tsconfig.tslib.json && ncp package.cjs.json tslib/package.json",
    "bundle": "webpack",
    "clean": "rimraf cjs coverage dist esm lib libm tslib",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest --coverage",
    "depcheck": "depcheck",
    "lint": "eslint --ext=js,ts .",
    "prepublishOnly": "pnpm bundle",
    "nuke": "rimraf node_modules",
    "size": "size-limit",
    "test": "run-p test:node test:jsdom test:swc",
    "test:jsdom": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest -c jest.config.jsdom.mjs",
    "test:node": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest -c jest.config.mjs",
    "test:swc": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest -c jest.config.swc.mjs",
    "verify": "npm-run-all clean -p build depcheck lint coverage -p size",
    "watch": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest --watch"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "is-buffer": "^2.0.5",
    "unpartial": "^1.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@repobuddy/jest": "^5.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@size-limit/preset-small-lib": "^9.0.0",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-harmony": "^7.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config-2": "^2.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "ncp": "^2.0.0",
    "npm-run-all2": "^6.2.6",
    "param-case": "^4.0.0",
    "pascal-case": "^4.0.0",
    "rimraf": "^6.1.3",
    "size-limit": "^9.0.0",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "tslib": "^2.8.1",
    "type-plus": "^7.6.2",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "ws": "^8.19.0"
  }
}
