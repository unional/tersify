{
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"lib": ["DOM", "ES2019"],
		"moduleResolution": "node",
		"newLine": "LF",
		"noImplicitAny": false,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"rootDir": "src",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"stripInternal": true,
		"useDefineForClassFields": true
	},
	"files": ["src/index.ts", "src/constants.browser.ts", "src/tersifyFunction.browser.ts"]
}
